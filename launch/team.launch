<launch> 

<!-- Axes -->
    <param name="scale_linear" value="2" type="double"/>
    <param name="scale_angular" value="2" type="double"/>

  <node name="master" pkg="task_assign" type="central_node" args="7" output="screen"/>

  <node name="task1" pkg="task_assign" type="task_node" args="task1 1 5 7 8.5 -1.5" output="screen"/>
  <node name="task3" pkg="task_assign" type="task_node" args="task3 3 5 3 8.5 -1.5" output="screen"/>
  <node name="task4" pkg="task_assign" type="task_node" args="task4 4 5 5.54 5.54 -1.5" output="screen"/>
  <node name="task2" pkg="task_assign" type="task_node" args="task2 2 5 2 5 -1.5" output="screen"/>
  <node name="task5" pkg="task_assign" type="task_node" args="task5 5 5 8 4 -1.5" output="screen"/>
  <node name="task6" pkg="task_assign" type="task_node" args="task6 6 5 1 1 -1.5" output="screen"/>
  <node name="task7" pkg="task_assign" type="task_node" args="task7 7 5 10 10 -1.5" output="screen"/>
  <node name="task8" pkg="task_assign" type="task_node" args="task8 8 5 10 1 -1.5" output="screen"/>
 
  <node name="robot1" pkg="task_assign" type="robot_node" args="robot1 1 5.54 1 1.5" output="screen"/> 
  <node name="robot2" pkg="task_assign" type="robot_node" args="robot2 2 4 1 1.5" output="screen"/> 
  <node name="robot3" pkg="task_assign" type="robot_node" args="robot3 3 3 2 1.5" output="screen"/> 
  
</launch>




